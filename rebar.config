
{erl_opts, [debug_info,
            {i, "./_build/default/plugins/gpb/include"}
           ]}.

{deps, []}.

{plugins, [{rebar3_gpb_plugin, "2.22.1"}
          ]}.

{gpb_opts, [{i, "proto"},
            {o_erl, "src/pb"},
            {o_hrl, "include"},
            {module_name_prefix, "pb_"}
           ]}.

{provider_hooks, [
    {pre, [
           {compile, {protobuf, compile}},
           {clean, {protobuf, clean}}
          ]}
]}.

